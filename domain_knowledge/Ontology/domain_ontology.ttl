#################################################################
#    Object Properties
#################################################################

###  https://hpc-fair.github.io/ontology#actedOnBehalfOf
:actedOnBehalfOf rdf:type owl:ObjectProperty ;
                 rdfs:comment "Some agent acted on behalf of another agent." .


###  https://hpc-fair.github.io/ontology#after
:after rdf:type owl:ObjectProperty ;
       owl:inverseOf :before ;
       rdf:type owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#aiModel
:aiModel rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :data ;
         rdfs:comment "a project has an ai model" .


###  https://hpc-fair.github.io/ontology#associatedSoftware
:associatedSoftware rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#before
:before rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#benchmark
:benchmark rdf:type owl:ObjectProperty ;
           rdfs:comment "Link to the associated benchmark software ID" .


###  https://hpc-fair.github.io/ontology#conducted
:conducted rdf:type owl:ObjectProperty ;
           owl:inverseOf :wasConductedBy ;
           rdfs:comment "an agent conducted an activity" .


###  https://hpc-fair.github.io/ontology#consistsOf
:consistsOf rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#contributor
:contributor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :wasAttributeTo .


###  https://hpc-fair.github.io/ontology#coprocessor
:coprocessor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :processor ;
             rdfs:comment "a computer has a coprocessor" .


###  https://hpc-fair.github.io/ontology#correspondsToArtifactTemplate
:correspondsToArtifactTemplate rdf:type owl:ObjectProperty ;
                               rdfs:comment "Artifact of an activity corresponds to a artifact template of a plan. Similar to opmv:correspondsToTemplateArtifact." .


###  https://hpc-fair.github.io/ontology#correspondsToPlan
:correspondsToPlan rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some activity corresponds to (follows) a plan." .


###  https://hpc-fair.github.io/ontology#correspondsToStep
:correspondsToStep rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some activity corresponds to a step of a plan. Similar to ompw:correspondsToTemplateProcess." .


###  https://hpc-fair.github.io/ontology#cpu
:cpu rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf :processor ;
     rdfs:comment "a computer has a cpu object linked" .


###  https://hpc-fair.github.io/ontology#creator
:creator rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :contributor .


###  https://hpc-fair.github.io/ontology#data
:data rdf:type owl:ObjectProperty ;
      rdfs:comment "a project has data (dataset, aiModel ,etc). Derived from experiment generating or using the data." .


###  https://hpc-fair.github.io/ontology#dataset
:dataset rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :data ;
         rdfs:comment "a project has a dataset" .


###  https://hpc-fair.github.io/ontology#feed
:feed rdf:type owl:ObjectProperty ;
      rdfs:comment "link to software which use this data as input: inverse of hpc:input or hpc:use" .


###  https://hpc-fair.github.io/ontology#file
:file rdf:type owl:ObjectProperty ;
      rdfs:range :File ;
      rdfs:comment "a data object has an associated file" .


###  https://hpc-fair.github.io/ontology#fundedBy
:fundedBy rdf:type owl:ObjectProperty ;
          rdfs:comment "a project is funded by some agencies" .


###  https://hpc-fair.github.io/ontology#generated
:generated rdf:type owl:ObjectProperty ;
           owl:equivalentProperty :hadOutput ;
           owl:inverseOf :wasGeneratedBy ;
           rdfs:comment "Some activie generated some artifact" .


###  https://hpc-fair.github.io/ontology#generates
:generates rdf:type owl:ObjectProperty ;
           owl:inverseOf :wasGeneratedBy ;
           rdfs:comment "Some activity/experiment generates some data (workflow, dataset, ai models, etc. )" .


###  https://hpc-fair.github.io/ontology#graphLink
:graphLink rdf:type owl:ObjectProperty ;
           rdfs:comment "the top level object properties for any links between AST nodes" .


###  https://hpc-fair.github.io/ontology#hadInput
:hadInput rdf:type owl:ObjectProperty ;
          owl:equivalentProperty :used ;
          owl:inverseOf :wasUsedBy ;
          rdfs:comment "Link to ids of input datasets used to generate this model (dataset)" .


###  https://hpc-fair.github.io/ontology#hadOutput
:hadOutput rdf:type owl:ObjectProperty ;
           owl:inverseOf :wasGeneratedBy .


###  https://hpc-fair.github.io/ontology#hasCompiler
:hasCompiler rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasInput
:hasInput rdf:type owl:ObjectProperty ;
          owl:equivalentProperty :uses ;
          owl:inverseOf :isInputOf ;
          rdfs:range :ArtifactTemplate ;
          rdfs:comment "A plan or step has some input artifact template" .


###  https://hpc-fair.github.io/ontology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:range :Location ;
             rdfs:comment "The location information of a computer or an organization" .


###  https://hpc-fair.github.io/ontology#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:equivalentProperty :member ;
           owl:inverseOf :memberOf .


###  https://hpc-fair.github.io/ontology#hasNode
:hasNode rdf:type owl:ObjectProperty ;
         rdfs:comment "Link a cluster to its login or compute nodes." .


###  https://hpc-fair.github.io/ontology#hasOperatingSystem
:hasOperatingSystem rdf:type owl:ObjectProperty ;
                    owl:equivalentProperty :operatingSystem ;
                    rdfs:comment "Link from a machine to OS software ID: RHEL version 8, the one runs on the computer" .


###  https://hpc-fair.github.io/ontology#hasOutput
:hasOutput rdf:type owl:ObjectProperty ;
           owl:inverseOf :isOutputOf ;
           rdfs:range :ArtifactTemplate ;
           rdfs:comment "A plan or step has some artifact template" .


###  https://hpc-fair.github.io/ontology#hasPI
:hasPI rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasSize
:hasSize rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasVendor
:hasVendor rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#isArtifactTemplateOfPlan
:isArtifactTemplateOfPlan rdf:type owl:ObjectProperty ;
                          rdfs:comment "some artifact template belongs to a plan as input or output. simiar to opmw:isVariableOfTemplate" .


###  https://hpc-fair.github.io/ontology#isDecomposedAsPlan
:isDecomposedAsPlan rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:comment "MultiStep is decomposed to a Plan" .


###  https://hpc-fair.github.io/ontology#isDerivedFrom
:isDerivedFrom rdf:type owl:ObjectProperty ;
               rdfs:comment "some artifact template is derived from another artifact template, after some steps." .


###  https://hpc-fair.github.io/ontology#isGeneratedBy
:isGeneratedBy rdf:type owl:ObjectProperty ;
               owl:equivalentProperty :isOutputOf ;
               rdfs:domain :ArtifactTemplate ;
               rdfs:comment "Some artifact template is generated by a plan or step" .


###  https://hpc-fair.github.io/ontology#isInputOf
:isInputOf rdf:type owl:ObjectProperty ;
           rdfs:comment "some artifact template is an input of a plan or step" .


###  https://hpc-fair.github.io/ontology#isOutputOf
:isOutputOf rdf:type owl:ObjectProperty ;
            rdfs:comment "some artifact template is output of a plan or step" .


###  https://hpc-fair.github.io/ontology#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:comment "is a part of bigger organization entities, used to support parent-child organization relation" .


###  https://hpc-fair.github.io/ontology#isPreceededBy
:isPreceededBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Step ;
               rdfs:range :Step ;
               rdfs:comment "A step is preceeded by another step." .


###  https://hpc-fair.github.io/ontology#isStepOfPlan
:isStepOfPlan rdf:type owl:ObjectProperty ;
              rdfs:domain :Step ;
              rdfs:range :Plan ;
              rdfs:comment "a step is a step of a plan. Similar to opmw:isStepOfTemplate" .


###  https://hpc-fair.github.io/ontology#isSubPlanOfPlan
:isSubPlanOfPlan rdf:type owl:ObjectProperty ;
                 rdfs:comment "A plan is a sub plan of another plan.  Linking MultiStep to its parent Plan." .


###  https://hpc-fair.github.io/ontology#isVariableTemplateOfPlan
:isVariableTemplateOfPlan rdf:type owl:ObjectProperty ;
                          rdfs:comment "similar to opmw:isVariableOfPlan.  Links a variable template to its plan" .


###  https://hpc-fair.github.io/ontology#member
:member rdf:type owl:ObjectProperty ;
        owl:inverseOf :memberOf .


###  https://hpc-fair.github.io/ontology#memberOf
:memberOf rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#next
:next rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      owl:inverseOf :prev .


###  https://hpc-fair.github.io/ontology#operatingSystem
:operatingSystem rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#osFamily
:osFamily rdf:type owl:ObjectProperty ;
          rdfs:comment "Linux, MacOS, Windows etc." .


###  https://hpc-fair.github.io/ontology#prev
:prev rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty .


###  https://hpc-fair.github.io/ontology#processor
:processor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "a computer node links to its processor node" .


###  https://hpc-fair.github.io/ontology#programmingLanguage
:programmingLanguage rdf:type owl:ObjectProperty ;
                     rdfs:range :ProgrammingLanguage ;
                     rdfs:comment "software is written in some programming language" .


###  https://hpc-fair.github.io/ontology#project
:project rdf:type owl:ObjectProperty ;
         rdfs:comment "something has a project associated" .


###  https://hpc-fair.github.io/ontology#ranOn
:ranOn rdf:type owl:ObjectProperty ;
       rdfs:comment "Some software ran on some machine." .


###  https://hpc-fair.github.io/ontology#runsOn
:runsOn rdf:type owl:ObjectProperty ;
        owl:equivalentProperty :supportedMachine ;
        rdfs:range :Computer ;
        rdfs:comment "software runs on some machine" .


###  https://hpc-fair.github.io/ontology#software
:software rdf:type owl:ObjectProperty ;
          rdfs:comment "software used by a hardware devices." .


###  https://hpc-fair.github.io/ontology#submitter
:submitter rdf:type owl:ObjectProperty ;
           rdfs:comment "A person submits data into our system" .


###  https://hpc-fair.github.io/ontology#supportedMachine
:supportedMachine rdf:type owl:ObjectProperty ;
                  rdfs:comment "some software package supports some type of machines" .


###  https://hpc-fair.github.io/ontology#supportedOperatingSystem
:supportedOperatingSystem rdf:type owl:ObjectProperty ;
                          rdfs:comment "a software package supports some OS types" .


###  https://hpc-fair.github.io/ontology#targetApplication
:targetApplication rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:comment "target application an AI model supports" .


###  https://hpc-fair.github.io/ontology#targetMachine
:targetMachine rdf:type owl:ObjectProperty ;
               rdfs:comment "link to concrete computers an AI model supports to model"^^xsd:anyURI .


###  https://hpc-fair.github.io/ontology#used
:used rdf:type owl:ObjectProperty ;
      owl:inverseOf :wasUsedBy ;
      rdfs:comment "Activity/Experiment used some artifact as input" .


###  https://hpc-fair.github.io/ontology#uses
:uses rdf:type owl:ObjectProperty ;
      rdfs:range :ArtifactTemplate ;
      rdfs:comment "A plan or step uses some artifact template as input" .


###  https://hpc-fair.github.io/ontology#usesWorkflow
:usesWorkflow rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :correspondsToPlan ;
              rdfs:comment "Some activity/experiment uses a workflow (plan). This is similar to opmw:correspondsToTemplate which link an execution instance to its workflow template (plan)." .


###  https://hpc-fair.github.io/ontology#vendor
:vendor rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasAWH
:wasAWH rdf:type owl:ObjectProperty ;
        owl:equivalentProperty :wasAssociatedWithHardware .


###  https://hpc-fair.github.io/ontology#wasAWS
:wasAWS rdf:type owl:ObjectProperty ;
        owl:equivalentProperty :wasAssociatedWithSoftware .


###  https://hpc-fair.github.io/ontology#wasAssociatedWithHardware
:wasAssociatedWithHardware rdf:type owl:ObjectProperty ;
                           rdfs:comment "some activity was associated with some hardware, such as runOn" .


###  https://hpc-fair.github.io/ontology#wasAssociatedWithSoftware
:wasAssociatedWithSoftware rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasAttributeTo
:wasAttributeTo rdf:type owl:ObjectProperty ;
                rdfs:comment "Some artifact was attributed to an agent" .


###  https://hpc-fair.github.io/ontology#wasConductedBy
:wasConductedBy rdf:type owl:ObjectProperty ;
                owl:equivalentProperty :wasControlledBy ;
                rdfs:comment "some activity was conducted by an agent." .


###  https://hpc-fair.github.io/ontology#wasControlledBy
:wasControlledBy rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasDerivedFrom
:wasDerivedFrom rdf:type owl:ObjectProperty ;
                rdfs:comment "Some artifact was derived from another, after some processing. Simiar to prov-o:wasDerivedFrom" .


###  https://hpc-fair.github.io/ontology#wasDerivedFromDataset
:wasDerivedFromDataset rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :wasDerivedFrom ;
                       rdfs:comment "Some AI models were derived from datasets in the workflow pipeline" .


###  https://hpc-fair.github.io/ontology#wasDerivedFromSoftware
:wasDerivedFromSoftware rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :wasDerivedFromDataset ;
                        rdfs:comment "program analysis information (such as a call graph) was derivd from an input software" .


###  https://hpc-fair.github.io/ontology#wasGeneratedBy
:wasGeneratedBy rdf:type owl:ObjectProperty ;
                rdfs:comment "Link from Artifact to the activity/experiments generating the data" ,
                             "Some artifact was generated by some activity." .


###  https://hpc-fair.github.io/ontology#wasPreceededBy
:wasPreceededBy rdf:type owl:ObjectProperty ;
                rdfs:comment "Some activity was preceeded by another activity." .


###  https://hpc-fair.github.io/ontology#wasUsedBy
:wasUsedBy rdf:type owl:ObjectProperty ;
           rdfs:comment "Link from artifact to experiment using it, reverse of hpc:used." .


###  https://hpc-fair.github.io/ontology#workflow
:workflow rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :data ;
          rdfs:comment "Project has some workflow" .


