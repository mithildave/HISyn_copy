#################################################################

###  http://purl.org/dc/elements/1.1/abstract
dc:abstract rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/contributor
dc:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
dc:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty ;
                rdfs:comment "Date when the vocabulary was created. There are different ways in which the date may be represented, but typically is with an xsd:dateTime literal. Note that some vocabularies, like schema.org, define their own object as range for the date property. We advise to look at the respective vocabulary specifications for more information." .


###  http://purl.org/dc/terms/issued
dcterms:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/doi
bibo:doi rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/status
bibo:status rdf:type owl:AnnotationProperty ;
            rdfs:comment "Indicates the status of the vocabulary (Recommnedation, Draft, etc.)" .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://schema.org/domainIncludes
schema:domainIncludes rdf:type owl:AnnotationProperty ;
                      rdfs:comment "one or more domains a property is associated with. Union semantics is used for multiple domains, instead of intersection semantics of rdfs:range" .


###  http://schema.org/rangeIncludes
schema:rangeIncludes rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdfs:comment "Definition of the term. We recommend it to be as precise as possible, in order to avoid possible misunderstandings when reusing the ontology." .


###  http://www.w3.org/2002/07/owl#priorVersion
owl:priorVersion rdf:type owl:AnnotationProperty ;
                 rdfs:comment "Adding a link to a previous version helps to see how the vocabulary has changed from one to the other. Thanks to this relationship, tools like WIDOCO may create automated changelog sections in the documentation of an ontology." .


###  http://www.w3.org/2002/07/owl#versionIRI
owl:versionIRI rdf:type owl:AnnotationProperty ;
               rdfs:comment "The IRI that identifies this version of your vocabulary." .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty ;
             rdfs:comment "It is always helpful to add examples on the intended usages of the ontology. Pointers to real examples are also valuable." .


#################################################################
#    Object Properties
#################################################################

###  https://hpc-fair.github.io/ontology#actedOnBehalfOf
hpc:actedOnBehalfOf rdf:type owl:ObjectProperty ;
                    rdfs:comment "Some agent acted on behalf of another agent." .


###  https://hpc-fair.github.io/ontology#after
hpc:after rdf:type owl:ObjectProperty ;
          owl:inverseOf hpc:before ;
          rdf:type owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#aiModel
hpc:aiModel rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:data ;
            rdfs:comment "a project has an ai model" .


###  https://hpc-fair.github.io/ontology#associatedSoftware
hpc:associatedSoftware rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#before
hpc:before rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty .


###  https://hpc-fair.github.io/ontology#benchmark
hpc:benchmark rdf:type owl:ObjectProperty ;
              rdfs:comment "Link to the associated benchmark software ID" .


###  https://hpc-fair.github.io/ontology#conducted
hpc:conducted rdf:type owl:ObjectProperty ;
              owl:inverseOf hpc:wasConductedBy ;
              rdfs:comment "an agent conducted an activity" .


###  https://hpc-fair.github.io/ontology#consistsOf
hpc:consistsOf rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#contributor
hpc:contributor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf hpc:wasAttributeTo .


###  https://hpc-fair.github.io/ontology#coprocessor
hpc:coprocessor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:processor ;
                rdfs:comment "a computer has a coprocessor" .


###  https://hpc-fair.github.io/ontology#correspondsToArtifactTemplate
hpc:correspondsToArtifactTemplate rdf:type owl:ObjectProperty ;
                                  rdfs:comment "Artifact of an activity corresponds to a artifact template of a plan. Similar to opmv:correspondsToTemplateArtifact." .


###  https://hpc-fair.github.io/ontology#correspondsToPlan
hpc:correspondsToPlan rdf:type owl:ObjectProperty ;
                      rdfs:comment "Some activity corresponds to (follows) a plan." .


###  https://hpc-fair.github.io/ontology#correspondsToStep
hpc:correspondsToStep rdf:type owl:ObjectProperty ;
                      rdfs:comment "Some activity corresponds to a step of a plan. Similar to ompw:correspondsToTemplateProcess." .


###  https://hpc-fair.github.io/ontology#cpu
hpc:cpu rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf hpc:processor ;
        rdfs:comment "a computer has a cpu object linked" .


###  https://hpc-fair.github.io/ontology#creator
hpc:creator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:contributor .


###  https://hpc-fair.github.io/ontology#data
hpc:data rdf:type owl:ObjectProperty ;
         rdfs:comment "a project has data (dataset, aiModel ,etc). Derived from experiment generating or using the data." .


###  https://hpc-fair.github.io/ontology#dataset
hpc:dataset rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:data ;
            rdfs:comment "a project has a dataset" .


###  https://hpc-fair.github.io/ontology#feed
hpc:feed rdf:type owl:ObjectProperty ;
         rdfs:comment "link to software which use this data as input: inverse of hpc:input or hpc:use" .


###  https://hpc-fair.github.io/ontology#file
hpc:file rdf:type owl:ObjectProperty ;
         rdfs:range hpc:File ;
         rdfs:comment "a data object has an associated file" .


###  https://hpc-fair.github.io/ontology#fundedBy
hpc:fundedBy rdf:type owl:ObjectProperty ;
             rdfs:comment "a project is funded by some agencies" .


###  https://hpc-fair.github.io/ontology#generated
hpc:generated rdf:type owl:ObjectProperty ;
              owl:equivalentProperty hpc:hadOutput ;
              owl:inverseOf hpc:wasGeneratedBy ;
              rdfs:comment "Some activie generated some artifact" .


###  https://hpc-fair.github.io/ontology#generates
hpc:generates rdf:type owl:ObjectProperty ;
              owl:inverseOf hpc:wasGeneratedBy ;
              rdfs:comment "Some activity/experiment generates some data (workflow, dataset, ai models, etc. )" .


###  https://hpc-fair.github.io/ontology#graphLink
hpc:graphLink rdf:type owl:ObjectProperty ;
              rdfs:comment "the top level object properties for any links between AST nodes" .


###  https://hpc-fair.github.io/ontology#hadInput
hpc:hadInput rdf:type owl:ObjectProperty ;
             owl:equivalentProperty hpc:used ;
             owl:inverseOf hpc:wasUsedBy ;
             rdfs:comment "Link to ids of input datasets used to generate this model (dataset)" .


###  https://hpc-fair.github.io/ontology#hadOutput
hpc:hadOutput rdf:type owl:ObjectProperty ;
              owl:inverseOf hpc:wasGeneratedBy .


###  https://hpc-fair.github.io/ontology#hasCompiler
hpc:hasCompiler rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasInput
hpc:hasInput rdf:type owl:ObjectProperty ;
             owl:equivalentProperty hpc:uses ;
             owl:inverseOf hpc:isInputOf ;
             rdfs:range hpc:ArtifactTemplate ;
             rdfs:comment "A plan or step has some input artifact template" .


###  https://hpc-fair.github.io/ontology#hasLocation
hpc:hasLocation rdf:type owl:ObjectProperty ;
                rdfs:range hpc:Location ;
                rdfs:comment "The location information of a computer or an organization" .


###  https://hpc-fair.github.io/ontology#hasMember
hpc:hasMember rdf:type owl:ObjectProperty ;
              owl:equivalentProperty hpc:member ;
              owl:inverseOf hpc:memberOf .


###  https://hpc-fair.github.io/ontology#hasNode
hpc:hasNode rdf:type owl:ObjectProperty ;
            rdfs:comment "Link a cluster to its login or compute nodes." .


###  https://hpc-fair.github.io/ontology#hasOperatingSystem
hpc:hasOperatingSystem rdf:type owl:ObjectProperty ;
                       owl:equivalentProperty hpc:operatingSystem ;
                       rdfs:comment "Link from a machine to OS software ID: RHEL version 8, the one runs on the computer" .


###  https://hpc-fair.github.io/ontology#hasOutput
hpc:hasOutput rdf:type owl:ObjectProperty ;
              owl:inverseOf hpc:isOutputOf ;
              rdfs:range hpc:ArtifactTemplate ;
              rdfs:comment "A plan or step has some artifact template" .


###  https://hpc-fair.github.io/ontology#hasPI
hpc:hasPI rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasSize
hpc:hasSize rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#hasVendor
hpc:hasVendor rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#isArtifactTemplateOfPlan
hpc:isArtifactTemplateOfPlan rdf:type owl:ObjectProperty ;
                             rdfs:comment "some artifact template belongs to a plan as input or output. simiar to opmw:isVariableOfTemplate" .


###  https://hpc-fair.github.io/ontology#isDecomposedAsPlan
hpc:isDecomposedAsPlan rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:comment "MultiStep is decomposed to a Plan" .


###  https://hpc-fair.github.io/ontology#isDerivedFrom
hpc:isDerivedFrom rdf:type owl:ObjectProperty ;
                  rdfs:comment "some artifact template is derived from another artifact template, after some steps." .


###  https://hpc-fair.github.io/ontology#isGeneratedBy
hpc:isGeneratedBy rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty hpc:isOutputOf ;
                  rdfs:domain hpc:ArtifactTemplate ;
                  rdfs:comment "Some artifact template is generated by a plan or step" .


###  https://hpc-fair.github.io/ontology#isInputOf
hpc:isInputOf rdf:type owl:ObjectProperty ;
              rdfs:comment "some artifact template is an input of a plan or step" .


###  https://hpc-fair.github.io/ontology#isOutputOf
hpc:isOutputOf rdf:type owl:ObjectProperty ;
               rdfs:comment "some artifact template is output of a plan or step" .


###  https://hpc-fair.github.io/ontology#isPartOf
hpc:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:comment "is a part of bigger organization entities, used to support parent-child organization relation" .


###  https://hpc-fair.github.io/ontology#isPreceededBy
hpc:isPreceededBy rdf:type owl:ObjectProperty ;
                  rdfs:domain hpc:Step ;
                  rdfs:range hpc:Step ;
                  rdfs:comment "A step is preceeded by another step." .


###  https://hpc-fair.github.io/ontology#isStepOfPlan
hpc:isStepOfPlan rdf:type owl:ObjectProperty ;
                 rdfs:domain hpc:Step ;
                 rdfs:range hpc:Plan ;
                 rdfs:comment "a step is a step of a plan. Similar to opmw:isStepOfTemplate" .


###  https://hpc-fair.github.io/ontology#isSubPlanOfPlan
hpc:isSubPlanOfPlan rdf:type owl:ObjectProperty ;
                    rdfs:comment "A plan is a sub plan of another plan.  Linking MultiStep to its parent Plan." .


###  https://hpc-fair.github.io/ontology#isVariableTemplateOfPlan
hpc:isVariableTemplateOfPlan rdf:type owl:ObjectProperty ;
                             rdfs:comment "similar to opmw:isVariableOfPlan.  Links a variable template to its plan" .


###  https://hpc-fair.github.io/ontology#member
hpc:member rdf:type owl:ObjectProperty ;
           owl:inverseOf hpc:memberOf .


###  https://hpc-fair.github.io/ontology#memberOf
hpc:memberOf rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#next
hpc:next rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf hpc:prev .


###  https://hpc-fair.github.io/ontology#operatingSystem
hpc:operatingSystem rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#osFamily
hpc:osFamily rdf:type owl:ObjectProperty ;
             rdfs:comment "Linux, MacOS, Windows etc." .


###  https://hpc-fair.github.io/ontology#prev
hpc:prev rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty .


###  https://hpc-fair.github.io/ontology#processor
hpc:processor rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:comment "a computer node links to its processor node" .


###  https://hpc-fair.github.io/ontology#programmingLanguage
hpc:programmingLanguage rdf:type owl:ObjectProperty ;
                        rdfs:range hpc:ProgrammingLanguage ;
                        rdfs:comment "software is written in some programming language" .


###  https://hpc-fair.github.io/ontology#project
hpc:project rdf:type owl:ObjectProperty ;
            rdfs:comment "something has a project associated" .


###  https://hpc-fair.github.io/ontology#ranOn
hpc:ranOn rdf:type owl:ObjectProperty ;
          rdfs:comment "Some software ran on some machine." .


###  https://hpc-fair.github.io/ontology#runsOn
hpc:runsOn rdf:type owl:ObjectProperty ;
           owl:equivalentProperty hpc:supportedMachine ;
           rdfs:range hpc:Computer ;
           rdfs:comment "software runs on some machine" .


###  https://hpc-fair.github.io/ontology#software
hpc:software rdf:type owl:ObjectProperty ;
             rdfs:comment "software used by a hardware devices." .


###  https://hpc-fair.github.io/ontology#submitter
hpc:submitter rdf:type owl:ObjectProperty ;
              rdfs:comment "A person submits data into our system" .


###  https://hpc-fair.github.io/ontology#supportedMachine
hpc:supportedMachine rdf:type owl:ObjectProperty ;
                     rdfs:comment "some software package supports some type of machines" .


###  https://hpc-fair.github.io/ontology#supportedOperatingSystem
hpc:supportedOperatingSystem rdf:type owl:ObjectProperty ;
                             rdfs:comment "a software package supports some OS types" .


###  https://hpc-fair.github.io/ontology#targetApplication
hpc:targetApplication rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:comment "target application an AI model supports" .


###  https://hpc-fair.github.io/ontology#targetMachine
hpc:targetMachine rdf:type owl:ObjectProperty ;
                  rdfs:comment "link to concrete computers an AI model supports to model"^^xsd:anyURI .


###  https://hpc-fair.github.io/ontology#used
hpc:used rdf:type owl:ObjectProperty ;
         owl:inverseOf hpc:wasUsedBy ;
         rdfs:comment "Activity/Experiment used some artifact as input" .


###  https://hpc-fair.github.io/ontology#uses
hpc:uses rdf:type owl:ObjectProperty ;
         rdfs:range hpc:ArtifactTemplate ;
         rdfs:comment "A plan or step uses some artifact template as input" .


###  https://hpc-fair.github.io/ontology#usesWorkflow
hpc:usesWorkflow rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf hpc:correspondsToPlan ;
                 rdfs:comment "Some activity/experiment uses a workflow (plan). This is similar to opmw:correspondsToTemplate which link an execution instance to its workflow template (plan)." .


###  https://hpc-fair.github.io/ontology#vendor
hpc:vendor rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasAWH
hpc:wasAWH rdf:type owl:ObjectProperty ;
           owl:equivalentProperty hpc:wasAssociatedWithHardware .


###  https://hpc-fair.github.io/ontology#wasAWS
hpc:wasAWS rdf:type owl:ObjectProperty ;
           owl:equivalentProperty hpc:wasAssociatedWithSoftware .


###  https://hpc-fair.github.io/ontology#wasAssociatedWithHardware
hpc:wasAssociatedWithHardware rdf:type owl:ObjectProperty ;
                              rdfs:comment "some activity was associated with some hardware, such as runOn" .


###  https://hpc-fair.github.io/ontology#wasAssociatedWithSoftware
hpc:wasAssociatedWithSoftware rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasAttributeTo
hpc:wasAttributeTo rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some artifact was attributed to an agent" .


###  https://hpc-fair.github.io/ontology#wasConductedBy
hpc:wasConductedBy rdf:type owl:ObjectProperty ;
                   owl:equivalentProperty hpc:wasControlledBy ;
                   rdfs:comment "some activity was conducted by an agent." .


###  https://hpc-fair.github.io/ontology#wasControlledBy
hpc:wasControlledBy rdf:type owl:ObjectProperty .


###  https://hpc-fair.github.io/ontology#wasDerivedFrom
hpc:wasDerivedFrom rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some artifact was derived from another, after some processing. Simiar to prov-o:wasDerivedFrom" .


###  https://hpc-fair.github.io/ontology#wasDerivedFromDataset
hpc:wasDerivedFromDataset rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf hpc:wasDerivedFrom ;
                          rdfs:comment "Some AI models were derived from datasets in the workflow pipeline" .


###  https://hpc-fair.github.io/ontology#wasDerivedFromSoftware
hpc:wasDerivedFromSoftware rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf hpc:wasDerivedFromDataset ;
                           rdfs:comment "program analysis information (such as a call graph) was derivd from an input software" .


###  https://hpc-fair.github.io/ontology#wasGeneratedBy
hpc:wasGeneratedBy rdf:type owl:ObjectProperty ;
                   rdfs:comment "Link from Artifact to the activity/experiments generating the data" ,
                                "Some artifact was generated by some activity." .


###  https://hpc-fair.github.io/ontology#wasPreceededBy
hpc:wasPreceededBy rdf:type owl:ObjectProperty ;
                   rdfs:comment "Some activity was preceeded by another activity." .


###  https://hpc-fair.github.io/ontology#wasUsedBy
hpc:wasUsedBy rdf:type owl:ObjectProperty ;
              rdfs:comment "Link from artifact to experiment using it, reverse of hpc:used." .


###  https://hpc-fair.github.io/ontology#workflow
hpc:workflow rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:data ;
             rdfs:comment "Project has some workflow" .


#################################################################
#    Data properties
#################################################################

###  https://hpc-fair.github.io/ontology#accuracy
hpc:accuracy rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#affiliate
hpc:affiliate rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#alternateName
hpc:alternateName rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf hpc:name .


###  https://hpc-fair.github.io/ontology#awardAmount
hpc:awardAmount rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#city
hpc:city rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#codeVariant
hpc:codeVariant rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#computeNodeCount
hpc:computeNodeCount rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#continent
hpc:continent rdf:type owl:DatatypeProperty ;
              rdfs:comment "located continent" .


###  https://hpc-fair.github.io/ontology#coprocessorCores
hpc:coprocessorCores rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#coprocessorCountPerNode
hpc:coprocessorCountPerNode rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#coprocessorMemorySize
hpc:coprocessorMemorySize rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf hpc:size .


###  https://hpc-fair.github.io/ontology#coprocessorMemorySizePerNode
hpc:coprocessorMemorySizePerNode rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf hpc:size .


###  https://hpc-fair.github.io/ontology#coprocessorPeakPerformancePerNode
hpc:coprocessorPeakPerformancePerNode rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf hpc:performance .


###  https://hpc-fair.github.io/ontology#coprocessorSpeed
hpc:coprocessorSpeed rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:frequency ;
                     rdfs:comment "The frequency of a coprocessor" .


###  https://hpc-fair.github.io/ontology#count
hpc:count rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#country
hpc:country rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#cpuArchitecture
hpc:cpuArchitecture rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#cpuCoreCount
hpc:cpuCoreCount rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#cpuCorePerNode
hpc:cpuCorePerNode rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#cpuCorePerSocket
hpc:cpuCorePerSocket rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:count .


###  https://hpc-fair.github.io/ontology#cpuFrequency
hpc:cpuFrequency rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf hpc:frequency .


###  https://hpc-fair.github.io/ontology#cpuMemorySizePerNode
hpc:cpuMemorySizePerNode rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf hpc:size .


###  https://hpc-fair.github.io/ontology#cpuPeakPerformancePerNode
hpc:cpuPeakPerformancePerNode rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf hpc:performance .


###  https://hpc-fair.github.io/ontology#date
hpc:date rdf:type owl:DatatypeProperty .


###  https://hpc-fair.github.io/ontology#dateCommissioned
hpc:dateCommissioned rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:date .


###  https://hpc-fair.github.io/ontology#description
hpc:description rdf:type owl:DatatypeProperty .


